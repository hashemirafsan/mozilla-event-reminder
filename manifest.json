{

  "manifest_version": 2,
  "name": "LU-reminder",
  "version": "1.0.2",

  "description": "Addons For Leading university event list",
  "icons": {
    "32":"icons/moz.png",
    "48": "icons/moz.png",
    "96": "icons/moz.png"
  },
  "author": "Hashemi Rafsan",
  "applications": {
  "gecko": {
    "id": "rafsan@freebuck.xyz",
    "strict_min_version": "42.0",
    "strict_max_version": "50.*"
    }
  },
  "homepage_url": "https://github.com/hashemirafsan/mozilla-event-reminder",
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["reminder.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/moz.png"
    }
  },
  "permissions": [
    "alarms",
    "*://*/*",
    "webRequest",
    "tabs"
  ]

}
